{
  "extends": "airbnb",
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "no-console": "off",
    "func-names": ["error", "never"],
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id",
          "__parseValue",
          "__serialize",
          "__parseLiteral",
        ]
      }
    ]
  }
}
